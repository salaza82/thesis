\newcommand{\hwcpsLLH}{
    \begin{equation} \label{eq:hwc_ps_llh}
        \mathcal{L}=\prod_i \frac{(B_i+S_i)^{N_i}e^{-(B_i+S_i)}}{N_i !}
    \end{equation}
}

\newcommand{\gdLHratio}{
    \begin{equation} \label{eq:gd_LLH_test}
        \lambda \left( \svtex \mid \bm{\mathcal{D}_{\text{dSphs}}} \right) = \frac{\mathcal{L} \left( \svtex ; \bm{\hat{\hat{\nu}}} \mid \bm{\mathcal{D}_{\text{dSphs}}} \right)}{\mathcal{L} \left( \widehat{\svtex} ; \bm{\hat{\nu}} \mid \bm{\mathcal{D}_{\text{dSphs}}} \right)}
    \end{equation}
}

\newcommand{\gdJointLLH}{
    \begin{equation} \label{eq:gdJointLLH}
    \small{
        \mathcal{L}_{lk} \left( \svtex ; J_l, \bm{\nu_{lk}} \mid \bm{\mathcal{D}_{lk}} \right) = \\
        \prod_{i=1}^{N_{\text{E'}}} \prod_{j=1}^{N_{\text{P'}}} \Big[ \mathcal{P} (s_{lk,ij}(\svtex, J_l, \bm{\nu_{lk}}) + b_{lk,ij}(\bm{\nu_{lk}}) \mid N_{lk,ij}) \Big] \\
        \times \mathcal{L}_{lk,\bm{\nu}} \left( \bm{\nu_{lk}} \mid \bm{\mathcal{D}_{\nu_{lk}}} \right)
    }
    \end{equation}
}

\newcommand{\gdExpectedNS}{
\begin{equation} \label{eq:gd_expected_ns} \small{
    s_{ij}(\svtex, J) = \int_{E'_{\text{min},i}}^{E'_{\text{max},i}} \text{d}E' \int_{P'_{\text{min},j}}^{P'_{\text{max},j}} \text{d}\Omega' \int_{0}^{\infty} \text{d}E \int_{\Delta\Omega_{tot}} \text{d}\Omega \int_{0}^{T_{\text{obs}}} \text{d}t \frac{\text{d}^{2}\Phi (\svtex, J)}{\text{d}E \text{d}\Omega} \text{IRF} \left( E', P' \mid E, P, t \right)
    }
\end{equation}
}

