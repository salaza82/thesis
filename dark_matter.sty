\newcommand{\gdHAWCCL}{
    \begin{equation}\label{eq:hawc_cl95}
        \mathrm{TS}_{95} \equiv \sum_{\mathrm{bins}} \left[2 N \ln \left(1+\frac{\epsilon S_{\mathrm{ref}}}{B} \right) - 2\epsilon S_{\mathrm{ref}} \right]
    \end{equation}
}

