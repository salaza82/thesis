}

\newcommand{\GDjointLLH}{
\begin{equation} \label{eq:measurement_combination}
    \mathcal{L}_{\mathrm{dSph},l}\left( \svtex ; J_l, \bm{\nu_l} \mid \bm{\mathcal{D}_{l}} \right) = \prod_{k=1}^{N_{\mathrm{exp},l}} \mathcal{L}_{lk} \left( \svtex ; J_l, \bm{\nu_{lk}} \mid \bm{\mathcal{D}_{lk}} \right),
