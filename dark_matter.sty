\newcommand{\iddmannilation}[2][\phi]{
    \begin{equation}\label{eq:id_dm_flux}
        \frac{d\Phi_#1}{dE_{#1}} = \frac{\svtex}{8 \pi m^2_{\chi}} \frac{dN_#1}{dE_#1} \times \int_\text{source}{d\Omega \int_{l.o.s}{\rho^2_\chi dl(r,\theta')}}
    \end{equation}
}

\newcommand{\iddmdecay}[2][\phi]{
    \begin{equation}\label{eq:id_dm_decay}
        \frac{d\Phi_#1}{dE_{#1}} = \frac{1}{4 \pi \tau m_{\chi}} \frac{dN_#1}{dE_#1} \times \int_\text{source}{d\Omega \int_{l.o.s}{\rho_\chi dl(r,\theta')}}
    \end{equation}
}

\newcommand{\jfactor}{
